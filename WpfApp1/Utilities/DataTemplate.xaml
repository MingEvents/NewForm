                    xmlns:vm="clr-namespace:WpfApp1.ViewModel"
                    xmlns:view="clr-namespace:WpfApp1.View">
    <DataTemplate DataType="{x:Type vm:CreateUserVM}">
        <view:CreateUser />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:CreateEventVM}">
